<!DOCTYPE html>
<html>
  <head>
    <title>CtrlApp</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'dycalendar', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'dashstyle', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'buttonanim', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', params[:controller] %>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
